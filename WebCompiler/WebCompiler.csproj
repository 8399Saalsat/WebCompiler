<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="package.csproj" Condition="Exists('package.csproj')" />
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFrameworks>netcoreapp3.1;net5.0</TargetFrameworks>
    <PackAsTool>true</PackAsTool>
    <ToolCommandName>webcompiler</ToolCommandName>
    <Nullable>enable</Nullable>
    <AssemblyName>Excubo.WebCompiler</AssemblyName>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="AutoprefixerHost" Version="3.0.13" />
    <PackageReference Include="DartSassHost" Version="1.0.0-preview6" />
    <PackageReference Include="JavaScriptEngineSwitcher.V8" Version="3.13.1" />
    <PackageReference Include="Microsoft.ClearScript.V8.Native.win-x86" Version="7.1.6" />
    <PackageReference Include="Microsoft.ClearScript.V8.Native.win-x64" Version="7.1.6" />
    <PackageReference Include="Microsoft.ClearScript.V8.Native.win-arm64" Version="7.1.6" />
    <PackageReference Include="Microsoft.ClearScript.V8.Native.linux-x64" Version="7.1.6" />
    <PackageReference Include="Microsoft.ClearScript.V8.Native.linux-arm" Version="7.1.6" />
    <PackageReference Include="Microsoft.ClearScript.V8.Native.linux-arm64" Version="7.1.6" />
    <PackageReference Include="Microsoft.ClearScript.V8.Native.osx-x64" Version="7.1.6" />
    <PackageReference Include="Microsoft.ClearScript.V8.Native.osx-arm64" Version="7.1.6" />
    <PackageReference Include="NUglify" Version="1.16.1" />
  </ItemGroup>
</Project>